<div class="row">
  <div class="span12">
    <div class="well">
      <ul class="users">
        <% @users.each do |user| %>
          <li>
            <%= image_tag avatar_url(user) %>
            <%= link_to user.name, user %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>